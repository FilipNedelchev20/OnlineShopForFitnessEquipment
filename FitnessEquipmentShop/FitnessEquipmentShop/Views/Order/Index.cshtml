@model IEnumerable<FitnessEquipmentShop.Data.Models.Entities.Order>

@{
    ViewData["Title"] = "My orders";
}

<h2>My orders</h2>
<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderDate.ToShortDateString()</td>
                <td>@order.Status</td>
            </tr>
        }
    </tbody>
</table>
